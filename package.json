{
    "name": "generator-jhipster-kotlin",
    "version": "1.1.0",
    "description": "A Kotlin based JHipster (KHipster)",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jhipster/jhipster-kotlin"
    },
    "keywords": [
        "kotlin",
        "java",
        "hipster",
        "yeoman-generator",
        "jhipster-blueprint"
    ],
    "author": "Sendil Kumar N <sendilkumarn@live.com> (https://sendilkumarn.js.org)",
    "bugs": {
        "url": "https://github.com/jhipster/jhipster-kotlin/issues"
    },
    "bin": {
        "khipster": "./cli/khipster.js"
    },
    "scripts": {
        "prettier:format": "prettier --write \"{,**/}*.{js,json,md,yml}\"",
        "completion": "tabtab install --name jhipster --auto",
        "lint": "npm run eslint && npm run ejs-lint",
        "lint-fix": "npm run prettier:format && npm run eslint -- --fix",
        "eslint": "eslint .",
        "ejs-lint": "npm run test:unit -- test/ejslint.js",
        "ejslint": "ejslint",
        "pretest": "npm run lint",
        "test": "npm run test:unit -- test/**/*.spec.js test/*.spec.js --no-insight",
        "test:unit": "mocha --timeout 120000 --slow 0 --reporter spec"
    },
    "dependencies": {
        "chalk": "2.4.2",
        "cheerio": "1.0.0-rc.2",
        "commander": "2.19.0",
        "didyoumean": "1.2.1",
        "ejs": "2.6.1",
        "generator-jhipster": "6.3.0",
        "glob": "7.1.3",
        "insight": "0.10.1",
        "jhipster-core": "4.2.0",
        "js-yaml": "3.13.1",
        "lodash": "4.17.13",
        "meow": "5.0.0",
        "mkdirp": "0.5.1",
        "pluralize": "7.0.0",
        "prettier": "1.16.4",
        "randexp": "0.5.3",
        "semver": "5.6.0",
        "shelljs": "0.8.3",
        "tabtab": "2.2.2",
        "yeoman-environment": "2.3.4",
        "yeoman-generator": "3.2.0"
    },
    "devDependencies": {
        "chai": "4.2.0",
        "ejs-lint": "0.3.0",
        "eslint": "5.14.1",
        "eslint-config-airbnb-base": "13.1.0",
        "eslint-config-prettier": "4.1.0",
        "eslint-plugin-import": "2.16.0",
        "eslint-plugin-prettier": "3.0.1",
        "fs-extra": "7.0.1",
        "mocha": "6.1.4",
        "yeoman-assert": "3.1.1",
        "yeoman-test": "1.9.1"
    },
    "resolutions": {
        "inquirer": "6.3.1"
    },
    "engines": {
        "node": ">=8.10.0",
        "npm": ">=5.6.0"
    },
    "license": "Apache-2.0",
    "homepage": "https://github.com/jhipster/jhipster-kotlin#readme"
}
